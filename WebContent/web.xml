<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
	<welcome-file-list>
		<welcome-file>HomePage.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<display-name>VTServlet</display-name>
        <servlet-name>VTServlet</servlet-name>
        <servlet-class>servlet.VTServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VTServlet</servlet-name>
        <url-pattern>/VTServlet</url-pattern>
    </servlet-mapping>

    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>

</web-app>